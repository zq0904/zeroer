{
  "name": "zeroer-core",
  "version": "0.1.1-beta.0",
  "author": {
    "name": "zhaoqi"
  },
  "bundleDependencies": [],
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -w -c rollup.config.js",
    "build": "npm run lint && npm run build:all",
    "build:all": "npm run build:cjs & npm run build:esm & npm run build:umd & npm run build:types",
    "build:cjs": "rm -rf ./cjs && cross-env BABEL_MODULES=cjs babel src -d cjs -s -x .js,.jsx,.ts,.tsx",
    "build:esm": "rm -rf ./esm && babel src -d esm -s -x .js,.jsx,.ts,.tsx",
    "build:umd": "rm -rf ./umd && cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:types": "rm -rf ./types && tsc --emitDeclarationOnly --declarationDir types",
    "lint": "npm run check:eslint & npm run check:types",
    "check:eslint": "eslint src --ext .js,.ts",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.8.7"
  },
  "deprecated": false,
  "description": "``` zeroer-core core library ```",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/standalone": "^7.7.6",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "autoprefixer": "^9.7.1",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "inquirer": "^6.5.2",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-browsersync": "^1.1.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "shelljs": "^0.8.3",
    "typescript": "^3.7.3"
  },
  "keywords": [],
  "license": "ISC",
